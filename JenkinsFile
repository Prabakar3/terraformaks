pipeline{
    agent any
    tools{
        maven 'm1'
    }
    stages{
        stage('test'){
        
            steps{
                sh 'mvn clean test'
            }
        }
        stage('install'){
            steps{
                sh 'mvn clean install -D skipTests=true'
		
            }
        }
	stage('SonarQube Analysis') {
          def mvn = tool 'm1';
          withSonarQubeEnv() {
            sh "${mvn}/bin/mvn clean verify sonar:sonar -Dsonar.projectKey=ram-sonar"
          }
        }

	stage("jfrog"){
            steps{
                rtUpload(
                    serverId: 'ram-jfrog',
                    spec: '''{
                        "files": [
                            {
                                "pattern": "**/**/*.*ar",
                                "target": "ram-jfrog/"
                            }
                        ]
                    }'''
                )
            }
        }
	stage('dockerbuild'){
	    steps{
  	            dir("backend"){
        	       sh 'docker build -t prabakars/backend:latest .'
                	}
		    }
	   steps{
                dir("frontend"){
                  sh 'docker build -t prabakars/frontend:latest .'
			        }
            }
    }
        
        stage('docker push'){
            steps{
                withCredentials([usernamePassword(credentialsId: 'docker', passwordVariable: 'pwd', usernameVariable: 'user')]) {
                    sh 'docker login -u raamriot -p Asdfghjkl;'74'
                    sh 'docker push raamriot/backend:latest'
                    sh 'docker push raamriot/frontend:latest'
                }
            }
  	}	
    }
}
