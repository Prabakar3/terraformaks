pipeline{
    agent any
    tools{
        maven 'm1'
    }
    stages{
        stage('test'){
        
            steps{
                sh 'mvn clean test'
            }
        }
        stage('install'){
            steps{
                sh 'mvn clean install -D skipTests=true'
		
            }
        }
	stage('SonarQube Analysis') {
          def mvn = tool 'Default Maven';
          withSonarQubeEnv() {
            sh "${mvn}/bin/mvn clean verify sonar:sonar -Dsonar.projectKey=ram-sonar"
          }
        }

	stage("jfrog"){
            steps{
                rtUpload(
                    serverId: 'ram-jfrog',
                    spec: '''{
                        "files": [
                            {
                                "pattern": "**/**/*.*ar",
                                "target": "ram-jfrog/"
                            }
                        ]
                    }'''
                )
            }
        }
    }
}
